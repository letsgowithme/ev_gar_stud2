<form method="post" action="{{ path('car.delete', {'id': car.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ car.id) }}">
    <button class="btn link_delete text-center">Supprimer</button>
</form>
