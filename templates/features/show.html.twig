{% extends 'base.html.twig' %}

{% block title %}Features{% endblock %}

{% block body %}
    <h1>Features</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feature.id }}</td>
            </tr>
            <tr>
                <th>Width</th>
                <td>{{ feature.width }}</td>
            </tr>
            <tr>
                <th>Length</th>
                <td>{{ feature.length }}</td>
            </tr>
            <tr>
                <th>Height</th>
                <td>{{ feature.height }}</td>
            </tr>
            <tr>
                <th>Weight</th>
                <td>{{ feature.weight }}</td>
            </tr>
            <tr>
                <th>Price_min</th>
                <td>{{ feature.priceMin }}</td>
            </tr>
            <tr>
                <th>Price_max</th>
                <td>{{ feature.priceMax }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_features_index') }}">back to list</a>

    <a href="{{ path('app_features_edit', {'id': feature.id}) }}">edit</a>

    {{ include('features/_delete_form.html.twig') }}
{% endblock %}
